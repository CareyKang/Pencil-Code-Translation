# The top level Makefile

all:	
	cd Client 	&& $(MAKE)
	cd Server	&& $(MAKE)
	@echo 
	@echo "*********************************************"
	@echo "All files Built"
	@echo "Please move to exe directory"
	@echo "*********************************************"
	@echo
clean:
	cd Client 	&& $(MAKE) clean
	cd Server	&& $(MAKE) clean
